{
  "manifest_version": 3,
  "name": "Skill Tracker",
  "version": "1.0",
  "description": "Track and manage employee skills with ease.",
  "permissions": [
    "alarms",
    "storage",
    "notifications",
    "scripting",
    "activeTab"
  ],
  "host_permissions": ["http://localhost:5000/slack/extract-skills"],
  "background": {
    "service_worker": "background.js"
  }
}
